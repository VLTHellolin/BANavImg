{
  "name": "my-vue-template",
  "version": "0.0.0",
  "license": "MPLv2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "serve": "vercel dev",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore && npx stylelint \"**/*.styl\"",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^3.0.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vercel/node": "^3.0.26",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/language-plugin-pug": "^2.0.12",
    "@vue/tsconfig": "^0.5.1",
    "cookie": "^0.6.0",
    "dayjs": "^1.11.10",
    "dexie": "^4.0.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.24.1",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.0.3",
    "pug": "^3.0.2",
    "serverless-kit": "^0.4.0",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-stylus": "^1.0.0",
    "stylus": "^0.63.0",
    "typescript": "~5.4.0",
    "vercel": "^33.7.0",
    "vite": "^5.1.6",
    "vue-pug-syntax": "^1.0.1",
    "vue-tsc": "^2.0.6"
  }
}
